@helper Tags(System.Web.Mvc.HtmlHelper htmlHelper,
        IEnumerable<JustBlog.Core.Objects.Tag> tags)
    {
        foreach (var tag in tags)
        {
            <div class="tags-div">
                @JustBlog.HtmlHelpers.ActionLinkExtensions.TagLink(htmlHelper, tag)
            </div>
        }
}